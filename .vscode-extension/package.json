{
	"name": "caagency-git-extension",
	"displayName": "CA Agency Git Extension",
	"description": "Extension using Git API",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.getCurrentBranch",
				"title": "Get Current Branch"
			}
		]
	},
	"extensionDependencies": [
		"vscode.git"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.74.0",
		"@types/node": "^20.0.0",
		"typescript": "^5.0.0"
	}
}
